{
  "edit_predictions": {
    "mode": "subtle"
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "audio": {
    "experimental.rodio_audio": true
  },
  "restore_on_startup": "none",
  "minimum_contrast_for_highlights": 51.0,
  "completions": {
    "lsp": true,
    "lsp_insert_mode": "replace"
  },
  "snippet_sort_order": "top",
  "agent": {
    "expand_edit_card": false,
    "always_allow_tool_actions": false,
    "model_parameters": [],
    "enable_feedback": false
  },
  "disable_ai": false,
  "calls": {
    "share_on_join": false,
    "mute_on_join": true
  },
  "buffer_font_family": "JetBrains Mono NL",
  "icon_theme": "Catppuccin Mocha",
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "ui_font_size": 16,
  "buffer_font_size": 15,
  "theme": "Catppuccin Mocha",
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "languages": {
    "Python": {
      "language_servers": ["ty", "ruff"]
    },
    "Shell Script": {
      "language_servers": ["bash-language-server"],
      "tab_size": 2,
      "hard_tabs": false
    }
  },
  "lsp": {
    "bash-language-server": {},
    "ruff": {
      "initialization_options": {
        "settings": {
          "exclude": ["*"]
        }
      }
    }
  },
  "auto_install_extensions": {
    "catppuccin": true,
    "catppuccin-icons": true,
    "html": true,
    "toml": true,
    "dockerfile": true,
    "git-firefly": true
  }
}
